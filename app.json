{
  "expo": {
    "name": "mottu_challenge",
    "slug": "gef-mottu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#05AF31",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Mottu Challenge"
    },
    "extra": {
      "eas": {
        "projectId": "513490f3-1d7c-466c-bd45-b115e37fe66e"
      }
    },
    "owner": "fesilva2109",
    "android": {
      "package": "com.fesilva2109.gefmottu"
    }
  }
}
