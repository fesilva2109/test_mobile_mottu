{
  "expo": {
    "name": "mottu_challenge",
    "slug": "mottu_challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser", "expo-notifications"],
    "experiments": {
      "typedRoutes": true
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#05AF31",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Mottu Challenge"
    },
    "extra": {
      "eas": {
        "projectId": "64339743069"
      }
    }
  }
}
